{
  "suggest.noselect": false,
  "suggest.preferCompleteThanJumpPlaceholder": true,
  "languageserver": {
    "terraform": {
      "command": "terraform-ls",
      "args": ["serve"],
      "filetypes": [
        "terraform",
        "tf"
      ],
      "initializationOptions": {},
      "settings": {}
    },
    "liquid": {
      "command": "theme-check-language-server",
      "filetypes": [
        "liquid"
      ],
      "settings": {
        "themeCheck": {
          "checkOnOpen": true,
          "checkOnChange": true,
          "checkOnSave": true
        }
      },
      "rootPatterns": [
        ".theme-check.yml"
      ]
    },
    "csharp-ls": {
      "command": "csharp-ls",
      "filetypes": ["cs"],
      "rootPatterns": ["*.csproj", ".vim/", ".git/", ".hg/"]
    },
    "kotlin": {
        "command": "kotlin-language-server",
        "filetypes": ["kotlin"]
    }
  },
  "go.goplsOptions": {
    "completeUnimported": true
  }
}

